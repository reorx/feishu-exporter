{
  "name": "Feishu Exporter",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Export Feishu docs with ease",
  "author": "Reorx <novoreorx@gmail.com>",
  "icons": {
    "16": "logo-580.png",
    "48": "logo-580.png",
    "128": "logo-580.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Export current or selected Feishu doc(s)",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      }
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ]
}
